{
  "name": "casket",
  "description": "Feathers CMS",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "main": "src",
  "type": "module",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Steffan",
    "email": "steffanhalv@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && NODE_OPTIONS=--experimental-vm-modules npm run jest",
    "lint": "eslint src/. test/. --config .eslintrc.json --fix",
    "dev": "nodemon --ignore admin/ src/",
    "start": "npm run build; pm2 start src/index.js -n casket -f --watch; pm2 save",
    "restart": "pm2 restart casket",
    "remove": "pm2 delete casket",
    "stop": "pm2 stop casket",
    "startup": "pm2 startup > startup.tmp; tail -n 1 startup.tmp > startup.sh; sh startup.sh; rm startup.tmp",
    "unstartup": "pm2 unstartup > unstartup.tmp; tail -n 1 unstartup.tmp > unstartup.sh; sh unstartup.sh; rm unstartup.tmp",
    "jest": "jest --forceExit",
    "build": "npm i && cd admin && npm i && npm run build"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "dependencies": {
    "@feathersjs/authentication": "^5.0.0-pre.22",
    "@feathersjs/authentication-local": "^5.0.0-pre.22",
    "@feathersjs/authentication-oauth": "^5.0.0-pre.22",
    "@feathersjs/configuration": "^5.0.0-pre.22",
    "@feathersjs/errors": "^5.0.0-pre.22",
    "@feathersjs/express": "^5.0.0-pre.22",
    "@feathersjs/feathers": "^5.0.0-pre.22",
    "@feathersjs/socketio": "^5.0.0-pre.22",
    "@feathersjs/transport-commons": "^5.0.0-pre.22",
    "@socket.io/mongo-adapter": "^0.2.1",
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "feathers-authentication-hooks": "^1.0.2",
    "feathers-hooks-common": "^5.0.0",
    "feathers-mongoose": "^8.5.1",
    "feathers-permissions": "^2.1.4",
    "helmet": "^5.1.0",
    "mongodb": "^4.6.0",
    "mongodb-core": "^3.2.7",
    "mongoose": "^6.3.5",
    "mongoose-intl": "^3.3.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.7.2"
  }
}
