{
  "name": "casket",
  "description": "Feathers CMS",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "main": "src",
  "type": "module",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Steffan",
    "email": "steffanhalv@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && NODE_OPTIONS=--experimental-vm-modules npm run jest",
    "lint": "eslint src/. test/. --config .eslintrc.json --fix",
    "dev": "NODE_OPTIONS=--experimental-vm-modules nodemon --ignore 'admin/' --ignore 'temp/' src/",
    "start": "node ./scripts/start.js",
    "stop": "node ./scripts/stop.js",
    "restart": "node ./scripts/restart.js",
    "delete": "node ./scripts/delete.js",
    "startup": "pm2 startup > startup.tmp; tail -n 1 startup.tmp > startup.sh; sh startup.sh; rm startup.tmp",
    "unstartup": "pm2 unstartup > unstartup.tmp; tail -n 1 unstartup.tmp > unstartup.sh; sh unstartup.sh; rm unstartup.tmp",
    "update": "git pull; npm run unstartup; npm run delete; npm run start; npm run startup",
    "jest": "jest --forceExit",
    "build": "npm i && cd admin && npm i && npm run build",
    "docbuild": "docker compose up -d --build",
    "docstart": "docker compose start",
    "docstop": "docker compose stop",
    "docremove": "docker compose down -v"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "dependencies": {
    "@feathersjs/authentication": "^5.0.0-pre.22",
    "@feathersjs/authentication-local": "^5.0.0-pre.22",
    "@feathersjs/authentication-oauth": "^5.0.0-pre.22",
    "@feathersjs/configuration": "^5.0.0-pre.22",
    "@feathersjs/errors": "^5.0.0-pre.22",
    "@feathersjs/express": "^5.0.0-pre.22",
    "@feathersjs/feathers": "^5.0.0-pre.22",
    "@feathersjs/socketio": "^5.0.0-pre.22",
    "@feathersjs/transport-commons": "^5.0.0-pre.22",
    "@root/csr": "^0.8.1",
    "@root/keypairs": "^0.10.3",
    "@root/pem": "^1.0.4",
    "@socket.io/mongo-adapter": "^0.2.1",
    "acme": "^3.0.3",
    "acme-http-01-webroot": "^3.0.0",
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "feathers-authentication-hooks": "^1.0.2",
    "feathers-hooks-common": "^5.0.0",
    "feathers-mongoose": "^8.5.1",
    "feathers-permissions": "^2.1.4",
    "googleapis": "^105.0.0",
    "helmet": "^5.1.0",
    "http-proxy": "^1.18.1",
    "moment": "^2.29.4",
    "mongodb": "^4.6.0",
    "mongodb-core": "^3.2.7",
    "mongodb-snapshot": "^1.4.1",
    "mongoose": "^6.3.5",
    "mongoose-intl": "^3.3.0",
    "punycode": "^2.1.1",
    "serve-favicon": "^2.5.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.19",
    "pm2": "^5.2.0"
  }
}
